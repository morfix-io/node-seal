{
  "name": "node-seal",
  "version": "4.3.10",
  "description": "Homomorphic Encryption in JavaScript using Microsoft SEAL",
  "repository": {
    "type": "git",
    "url": "https://github.com/morfix-io/node-seal"
  },
  "homepage": "https://morfix.io/sandbox",
  "author": {
    "name": "Nick Angelou",
    "email": "accounts@morfix.io",
    "url": "https://morfix.io"
  },
  "keywords": [
    "homomorphic",
    "encryption",
    "homomorphic-encryption",
    "microsoft seal",
    "web assembly",
    "fhe",
    "crypto",
    "cryptography",
    "cryptosystem"
  ],
  "main": "dist/throws_transparent/node/wasm/index.js",
  "module": "dist/throws_transparent/node/wasm/index.js",
  "browser": "dist/throws_transparent/web/wasm/index.js",
  "files": [
    "dist/throws_transparent/web/js/index.js",
    "dist/throws_transparent/web/js/index.js.map",
    "dist/throws_transparent/web/wasm/index.js",
    "dist/throws_transparent/web/wasm/index.js.map",
    "dist/throws_transparent/node/js/index.js",
    "dist/throws_transparent/node/js/index.js.map",
    "dist/throws_transparent/node/wasm/index.js",
    "dist/throws_transparent/node/wasm/index.js.map",
    "dist/allows_transparent/web/js/index.js",
    "dist/allows_transparent/web/js/index.js.map",
    "dist/allows_transparent/web/wasm/index.js",
    "dist/allows_transparent/web/wasm/index.js.map",
    "dist/allows_transparent/node/js/index.js",
    "dist/allows_transparent/node/js/index.js.map",
    "dist/allows_transparent/node/wasm/index.js",
    "dist/allows_transparent/node/wasm/index.js.map"
  ],
  "license": "MIT",
  "scripts": {
    "start:wasm": "export NODE_ENV=development && export ENVIRONMENT=web && export TARGET=wasm && webpack-dev-server --host 0.0.0.0",
    "start:js": "export NODE_ENV=development && export ENVIRONMENT=web && export TARGET=js && webpack-dev-server --host 0.0.0.0",
    "eslint-check": "eslint --print-config ./src/target/index.js | eslint-config-prettier-check",
    "docs": "rm -rf ./docs/* && jsdoc -c jsdoc.json",
    "build": "npm run build:web && npm run build:node",
    "build:web": "export ENVIRONMENT=web && export NODE_ENV=production && npm run build:wasm && npm run build:js",
    "build:node": "export ENVIRONMENT=node && export NODE_ENV=production && npm run build:wasm && npm run build:js",
    "build:wasm": "export TARGET=wasm && eslint ./src/target/wasm && webpack",
    "build:js": "export TARGET=js && eslint ./src/target/js && webpack",
    "build:clean": "rm -rf ./dist/*",
    "benchmark:bfv": "node ./benchmark/exampleBfvPerformanceDefault.js",
    "benchmark:ckks": "node ./benchmark/exampleCkksPerformanceDefault.js",
    "submodule:update": "bash scripts/submodule-update.sh",
    "em:update": "bash scripts/em-update.sh",
    "em:init": "bash scripts/em-init.sh",
    "seal:clean": "bash scripts/seal-clean.sh",
    "seal:cmake": "bash scripts/seal-cmake.sh",
    "seal:make": "bash scripts/seal-make.sh",
    "seal:build": "npm run seal:build:wasm && npm run seal:build:js",
    "seal:build:js": "bash scripts/seal-build-js.sh",
    "seal:build:wasm": "bash scripts/seal-build-wasm.sh",
    "seal:compile": "npm run seal:clean && npm run seal:cmake && npm run seal:make && npm run seal:build",
    "build:transparent:on": "export THROW_ON_TRANSPARENT=OFF && npm run seal:compile && npm run build",
    "build:transparent:off": "export THROW_ON_TRANSPARENT=ON && npm run seal:compile && npm run build",
    "build:all": "npm run build:transparent:on && npm run build:transparent:off",
    "clean": "npm run seal:clean && npm run build:clean",
    "compile": "tsc",
    "format": "prettier --write src",
    "lint": "eslint --fix 'src/**/*.ts'",
    "test": "jest src/**/*.ts",
    "test:js": "jest tsc-out/__tests__/*.js",
    "coverage": "jest src --coverage",
    "coverage:upload": "env-cmd -f .env codecov"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-typescript": "^6.0.0",
    "@types/jest": "^26.0.14",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jsdoc": "^30.6.4",
    "eslint-plugin-prettier": "^3.1.4",
    "esm": "^3.2.25",
    "jest": "^26.5.2",
    "path": "^0.12.7",
    "prettier": "^2.1.2",
    "rollup": "^2.29.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3",
    "better-docs": "^2.3.2",
    "codecov": "^3.8.0",
    "env-cmd": "^10.1.0",
    "jsdoc": "^3.6.6"
  },
  "browserslist": [
    "since 2017-06"
  ]
}
